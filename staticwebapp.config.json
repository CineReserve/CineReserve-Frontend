{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "*.css",
      "*.js",
      "*.mjs",
      "*.map",
      "*.ico",
      "*.png",
      "*.jpg",
      "*.jpeg",
      "*.svg",
      "*.webp",
      "*.woff",
      "*.woff2"
    ]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".json": "application/json",
    ".wasm": "application/wasm"
  }
}
